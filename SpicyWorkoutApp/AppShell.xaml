<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="SpicyWorkoutApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:SpicyWorkoutApp"
    xmlns:views="clr-namespace:SpicyWorkoutPlaner.Planer.Views;assembly=SpicyWorkoutPlaner.Planer"
    Shell.FlyoutBehavior="Flyout">

    <FlyoutItem Route="mainpage">
        <ShellContent ContentTemplate="{DataTemplate views:CreateWorkoutPage}" Route="create-workouts" />
    </FlyoutItem>

    <FlyoutItem Route="mainpage">
        <ShellContent ContentTemplate="{DataTemplate views:DisclaimerPage}" Route="disclaimer" />
    </FlyoutItem>

</Shell>
